drawBoard = (ctx, grid, size) ->
  ctx.beginPath
  for i in [1..grid]
    ctx.moveTo(i * size, size)
    ctx.lineTo(i * size, grid * size)
    ctx.moveTo(size, i * size)
    ctx.lineTo(grid * size, i * size)
  ctx.stroke()

canvas = document.getElementById("board")
ctx = canvas.getContext('2d')

drawBoard(ctx, 19, 50)
